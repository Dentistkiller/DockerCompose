@model List<Client.Models.WeatherForecast>

<h1>Weather Forecast</h1>

<table class="table table-striped">
    <thead>
        <tr>
            <th>Date</th>
            <th>Temp (°C)</th>
            <th>Summary</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var forecast in Model)
        {
            <tr>
                <td>@forecast.Date</td>
                <td>@forecast.TemperatureC</td>
                <td>@forecast.Summary</td>
            </tr>
        }
    </tbody>
</table>
